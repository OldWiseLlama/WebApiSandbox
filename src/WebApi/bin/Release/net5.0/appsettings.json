{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "HostContext": {
    "PodName": "N/A"
  },
  "LogOptions": {
    "WriteToConsole": true,
    "WriteToHttp": false,
    "HttpUri": ""
  },
  "FeatureManagement": {
    "UseAwesomeFeature": true,
    "UseInProgressFeature": false
  }
}
